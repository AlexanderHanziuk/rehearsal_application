{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
			"ESNext",
			"DOM",
			"DOM.Iterable"
		],
    "module": "ESNext",
    "moduleResolution": "node",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
  },
	"exclude": [
		"node_modules"
	],
	"include": [
		"src"
	]
}